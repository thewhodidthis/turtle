{
  "name": "@thewhodidthis/taxi",
  "version": "0.0.1",
  "description": "Helps with turtle graphics",
  "main": "index",
  "module": "index.mjs",
  "homepage": "https://github.com/thewhodidthis/taxi#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thewhodidthis/taxi.git"
  },
  "bugs": {
    "url": "https://github.com/thewhodidthis/taxi/issues"
  },
  "scripts": {
    "pretest": "eslint test.js",
    "test": "rollup -c node:x -f iife test.js | kpow",
    "prestart": "eslint index.mjs",
    "start": "rollup -e @thewhodidthis/arithmetics,poltocar -f cjs index.mjs --no-interop > index.js",
    "preexample": "eslint example/index.js",
    "example": "cd example; rollup -c node:x -f iife index.js > bundle.js"
  },
  "eslintConfig": {
    "extends": "@thewhodidthis"
  },
  "engines": {
    "node": ">=6.11.0",
    "npm": ">=3.10.10"
  },
  "dependencies": {
    "@thewhodidthis/arithmetics": "^0.0.5",
    "poltocar": "^2.0.5"
  },
  "devDependencies": {
    "@thewhodidthis/eslint-config": "^0.0.16",
    "cutaway": "^0.0.7",
    "eslint": "^6.8.0",
    "kpow": "^0.1.4",
    "rollup": "^1.31.1",
    "rollup-config-x": "^1.0.4",
    "tapeless": "^0.0.9"
  },
  "keywords": [
    "canvas",
    "graphics"
  ],
