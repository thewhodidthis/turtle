{
  "name": "@thewhodidthis/taxi",
  "version": "0.0.0",
  "description": "Helps with turtle graphics",
  "main": "index",
  "module": "index.mjs",
  "homepage": "https://github.com/thewhodidthis/taxi#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thewhodidthis/taxi.git"
  },
  "bugs": {
    "url": "https://github.com/thewhodidthis/taxi/issues"
  },
  "scripts": {
    "pretest": "eslint test.js",
    "test": "rollup -c node:x -f iife test.js | kpow",
    "prestart": "eslint index.mjs",
    "start": "rollup -e @thewhodidthis/arithmetics,poltocar -f cjs --no-interop index.mjs | buble > index.js",
    "preexample": "eslint example/index.es",
    "example": "cd example; rollup -c node:x -f iife index.es | buble | cpp -P | grep -v '^//' | uniq > index.js"
  },
  "eslintConfig": {
    "extends": "@thewhodidthis"
  },
  "engines": {
    "node": ">=6.4.0",
    "npm": ">=3.10.3"
  },
  "dependencies": {
    "@thewhodidthis/arithmetics": "github:thewhodidthis/arithmetics",
    "poltocar": "github:thewhodidthis/poltocar"
  },
  "devDependencies": {
    "@thewhodidthis/eslint-config": "latest",
    "buble": "latest",
    "cutaway": "latest",
    "eslint": "latest",
    "rollup": "latest",
    "rollup-config-x": "latest",
    "tapeless": "latest"
  },
  "keywords": [
    "canvas",
    "graphics"
  ],
  "author": "sChzs BkgNns <sotirios.bakagiannis@gmail.com> (https://www.thewhodidthis.com)",
  "license": "Unlicense"
}
